@section scripts{
    <script>
        $(document).ready(function () {
            $.getJSON("/api/Personas", function(data)
            {
                $.each(data, function (key, value) {
                    $("tbody").append("<tr>" + "<td>" + value.Nombre + "</td>" + "<td>" + value.Email + "</td>" + "<td>" + value.Edad + "</td>" + "</tr>");
                })
                    
                
            })
        })
    </script>
    }
